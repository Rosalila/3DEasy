<p id="notice"><%= notice %></p>

<h1>Printing Sets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Printing hub</th>
      <th>Doges</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @printing_sets.each do |printing_set| %>
      <tr>
        <td><%= printing_set.name %></td>
        <td><%= printing_set.printing_hub_id %></td>
        <td><%= printing_set.doges %></td>
        <td><%= link_to 'Show', printing_set %></td>
        <td><%= link_to 'Edit', edit_printing_set_path(printing_set) %></td>
        <td><%= link_to 'Destroy', printing_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Printing Set', new_printing_set_path %>
